{
  "name": "hobot",
  "version": "0.0.3",
  "description": "Framework for easier development of complex chat bots",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "rm -rf dist && tsc -p . --lib es2017 --outDir dist",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "chatbot"
  ],
  "author": "Andrey Selin",
  "license": "SEE LICENSE IN LICENCE",
  "dependencies": {
    "@types/node": "^11.11.3",
    "bluebird": "^3.5.5",
    "nodemon": "^1.18.10",
    "telegraf": "^3.28.0",
    "telegraf-i18n": "^6.5.0",
    "ts-node": "^8.0.3",
    "socks-proxy-agent": "^4.0.2",
    "typescript": "^3.3.3333"
  }
}
